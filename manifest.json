{
	"manifest_version": 2,
	"name": "Casey's Chrome New Tab Page",
	"version": "1.0.0",
	"description": "New tab page for personal use.",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": ["tabs", "notifications"],
	"web_accessible_resources": ["icon-pomorodo-48.png"],
	"content_security_policy": "script-src 'unsafe-eval'; object-src 'self'"
}
