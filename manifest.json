{
	"manifest_version": 2,
	"name": "Casey's Chrome New Tab Page",
	"version": "1.0.0",
	"description": "New tab page for personal use.",
	"offline_enabled": true,
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": [
		"tabs",
		"notifications",
		"webRequest",
		"*://*.163.com/",
		"*://*.baidu.com/",
		"*://*.sina.com.cn/"],
	"web_accessible_resources": [],
	"content_security_policy": "script-src 'unsafe-eval'; object-src 'self';"
}
